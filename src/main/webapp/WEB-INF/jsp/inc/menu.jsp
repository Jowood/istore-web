  .downarrow { background: url("css/hmenu/imgplus/downarrow.gif") no-repeat 100% 100%; padding-right: 11px;display: block; }
  td.hover .downarrow { background-image: url("css/hmenu/imgplus/downarrow-hover.gif"); word-break:keep-all;}
  td.active .downarrow { background-image: url("css/hmenu/imgplus/uparrow.gif"); }

  a.logo { position: absolute; right: 10px; top: 35px; background: none; }
  body > a.logo { position: fixed; }
  a.logo:hover { background: none; }
